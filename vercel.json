{
  "rewrites": [
    {
      "source": "/auth/:path*",
      "destination": "https://a4-moetko.vercel.app/auth/:path*"
    },
    {
      "source": "/api/:path*",
      "destination": "https://a4-moetko.vercel.app/api/:path*"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
